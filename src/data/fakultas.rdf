        <fns:pstudi>Program Studi Agribisnis</fns:pstudi>
        <fns:pstudi>Program Studi Peternakan</fns:pstudi>
        <fns:pstudi>Program Studi Keteknikan Pertanian</fns:pstudi>
        <fns:pstudi>Program Studi Manajemen Sumber Daya Perairan</fns:pstudi>
        <fns:pstudi>Program Studi Ilmu dan Teknologi Pangan</fns:pstudi>
        <fns:alamat>Jl. Prof. A.Sofyan No.3, Kampus USU, Medan 20155</fns:alamat>
    </owl:Class>
